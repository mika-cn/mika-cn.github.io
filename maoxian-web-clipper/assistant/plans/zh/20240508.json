      },
      {
        "chAttr": {
          "type": "assign.from.value",
          "pick": ".vue-recycle-scroller__item-view",
          "attr": "data-mx-locked-style-transform",
          "value": "translateY(0px)"
        }
      },
      {
        "chAttr": {
          "type": "assign.from.first-child-attr",
          "pick": ".vue-recycle-scroller__item-view",
          "attr": "data-mx-index",
          "tAttr": "data-index"
        }
      }
    ],
    "contributors": [
      "Mika"
    ]
